*{
	margin : 0px;
	padding: 0px;
}
body{
	background-color:#f1f2f3;
	/*background-image: url(../../images/simple_dashed.png);*/
}
body, input, button, select, textarea, th, td {
  font-size: 14px;
  line-height: 1.5;
  font-family: Helvetica,'Microsoft YaHei',arial;
  color: #666;
}
body,body > div, body > header, body > article, body > footer {
  display: block;
}

a:link,a:visited{
  font-weight: bold;
  text-align: center;
  color: #000000;
  text-decoration: none;
}

 a:hover, a:active{
  color: #996633;
}


.heading a:link,.heading a:visited{
  padding-top: 20px;
  padding-bottom:17px; 
  color: #666666;
  text-align: center;
  text-decoration: none;
}

.heading a:hover,.heading a:active{
  color: #FFCC00;
  border-bottom:5px solid #FFCC00;
}

.header{
	position: relative;
	height: 100px;
	width: 100%;
	background-color: red;
}

.container{
	position: relative;
	  margin-top: 0px;
	  margin-left: 0px;
  	  width: 100%;
  	  height: 100%;
}


.list_video{
  z-index: 12;
  width: 94%;
	position: relative;
  padding-left: 6%;
  margin-top: 16px;
  padding-bottom: 8px;
	float: left;
	overflow: hidden;
}

@media screen and (max-width: 1262px){
.dt-album {
  width: 978px;
  height: 328px;
}
}


.b_pic{
  height: 25px;
  width: 40%;
  margin-top: -18px;
   margin-left: 30%;
   background-image: url(../../images/ugbg.png);
   background-size:cover;
   padding-bottom: 15px;
}


.tName{
  position: relative;
  opacity: 0.5;
  font-weight: bold;
  font-size: 30px;
  margin-left: 25%;
  width: 50%; 
  text-align: center;
  margin-top: 12px;
  background: #666;
  -webkit-background-clip: text;
 -moz-background-clip: text;
 background-clip: text;
  color: transparent;
  text-shadow: 0 3px 3px rgba(255,255,255,0.5);


}

.list_video .section{
  border-radius:2px;
	position: relative;
  	width: 224px;
  	padding-bottom: 1px;
  	margin-right: 20px;
  	float: left;
  	margin-top: 22px;
  	box-shadow: 5px 5px 2px #888888;
}

.section-img {

  position: relative;
  cursor: pointer;
}
.section-img, .section-img img {
  border-radius:2px;
  display: block;
  width: 224px;
  height: 224px;
}

.section-desc {
  width: 222px;
  height: 80px;
  background-color: #ffffff;
  border-left: 1px solid #dedfe0;
  border-right: 1px solid #dedfe0;
  border-bottom: 1px solid #dedfe0;
}

.dt-username{
	font-size: 10px;
	opacity: 0.6;
}

.section-attr {
  height: 20px;
  padding: 0 10px;
  font-size: 12px;
  color: #888888;
  padding-top: 16px;
  padding-bottom: 12px;
}


.footer{
	position: relative;
	width: 100%;
	height:100px;
	margin-top: 1380px;

  z-index: 12;
}





/**********************Heading部分****************************/
.heading{
  margin-top: 0px;
  width: 100%;
  height: 65px;
  background-color: #fff;
  -moz-box-shadow:0px 0px 1px #000000 inset;            
  -webkit-box-shadow:0px 0px 1px #000000 inset;         
  box-shadow:0px 0px 1px #000000 inset; 
}

.heading_title{
  float:left;
  font-family: tahoma,arial,sans-serif;
  font-size: 42px;
  color: #FFFF33;
  margin-top: 0px;
  margin-left: 60px;
}

.heading_nav{
  width:100%;
  height:30px;
  position:relative;
}

@media screen and (min-width: 1231px) { 
.heading_navbar {margin-left: 300px;} 
} 

@media screen and (max-width: 1200px) { 
.heading_navbar {margin-left: 150px;} 
} 

@media screen and (max-width: 1100px) { 
.heading_navbar {margin-left: -150px;}
.heading_title{display: none;}
} 



.heading ul{
  margin-left: 140px;
  float: left;
  list-style-type: none;
  display: block;
}

.heading li{
  display: block;
  margin-top: 20px;
  height: 40px;
  margin-left: 35px;
  margin-right: 35px;
  float: left;
  font-size: 17px;
  text-align:center;
}

.titleLi{
  color: #FFCC00;
  border-bottom:5px solid #FFCC00;
}

.heading_search{
  margin-top: -2px;
  float: right;
  height: auto;
  width: auto;
  max-width: 100%;
  overflow: hidden;
  background: transparent;
  position: relative;
  margin-right: 50px;
}

.flexsearch--form {
  overflow: hidden;
  position: relative;

}
  
.flexsearch--input-wrapper {
  padding: 0 66px 0 0; 
  overflow: hidden;
}

.flexsearch--input {
  width: 100%;
  background: transparent;
}

.flexsearch--input {
    -webkit-box-sizing: content-box;
  -moz-box-sizing: content-box;
  box-sizing: content-box;
  height: 36px;
    padding: 0 46px 0 10px;
  border-color: #ffff66;
    border-radius: 35px; /* (height/2) + border-width */
  border-style: solid;
  border-width: 1px;
  margin-top: 15px;
  color: #333;
  font-family: "Segoe UI", "Lucida Grande", Helvetica, Arial, "Microsoft YaHei", FreeSans, Arimo, "Droid Sans", "wenquanyi micro hei", "Hiragino Sans GB", "Hiragino Sans GB W3", "FontAwesome", sans-serif;
  font-size: 14px;
  -webkit-appearance: none;
  -moz-appearance: none;
}
  
.flexsearch--submit {
  position: absolute;
  right: 0;
  top: 0;
  display: block;
  width: 38px;
  height: 30px;
  padding: 0;
  border: none;
  margin-top: 20px; /* margin-top + border-width */
  margin-right: 14px; /* border-width */
  background: transparent;
  color: #FFFF66;
  font-family: 'Helvetica', sans-serif;
  font-size: 22px;
  line-height: 20px;
}

.flexsearch--input:focus {
  outline: none;
  border-color: #CCFF66;
}

.flexsearch--input:focus.flexsearch--submit {
  color: #CCFF66; 
}

.flexsearch--submit:hover {
  color:#CCFF66;
  cursor: pointer;
}

::-webkit-input-placeholder {
  color: #FFFF33;  
}

input:-moz-placeholder {
  color: #FFFF33;
}

@media screen and (max-width: 780px) { 
.heading_navbar {margin-left: -150px;}
.heading_title{display: none;}
.heading_search{display: none;}
} 




/**************分页*********/
.meneame {
  padding-right: 3px; padding-left: 3px; font-size: 80%; padding-bottom: 3px; margin: 3px; color: #ff6500; padding-top: 3px; text-align: center
}
.meneame a {
  border-right: #ff9600 1px solid; padding-right: 7px; background-position: 50% bottom; border-top: #ff9600 1px solid; padding-left: 7px; background-image: url(meneame.jpg); padding-bottom: 5px; border-left: #ff9600 1px solid; color: #ff6500; margin-right: 3px; padding-top: 5px; border-bottom: #ff9600 1px solid; text-decoration: none
}
.meneame a:hover {
  border-right: #ff9600 1px solid; border-top: #ff9600 1px solid; background-image: none; border-left: #ff9600 1px solid; color: #ff6500; border-bottom: #ff9600 1px solid; background-color: #ffc794
}
.meneame a:active {
  border-right: #ff9600 1px solid; border-top: #ff9600 1px solid; background-image: none; border-left: #ff9600 1px solid; color: #ff6500; border-bottom: #ff9600 1px solid; background-color: #ffc794
}
.meneame span.current {
  border-right: #ff6500 1px solid; padding-right: 7px; border-top: #ff6500 1px solid; padding-left: 7px; font-weight: bold; padding-bottom: 5px; border-left: #ff6500 1px solid; color: #ff6500; margin-right: 3px; padding-top: 5px; border-bottom: #ff6500 1px solid; background-color: #ffbe94
}
.meneame span.disabled {
  border-right: #ffe3c6 1px solid; padding-right: 7px; border-top: #ffe3c6 1px solid; padding-left: 7px; padding-bottom: 5px; border-left: #ffe3c6 1px solid; color: #ffe3c6; margin-right: 3px; padding-top: 5px; border-bottom: #ffe3c6 1px solid
}







